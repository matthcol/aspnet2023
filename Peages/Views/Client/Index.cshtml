@model IEnumerable<Peages.Models.Client>
@{
    Layout = "_Layout";
}

<ul>
    @foreach (var client in @Model)
    {
   
        <li>@Html.ActionLink(client.Name, "ClientById", new {id = client.Id}, new {@class = "nav-link text-dark"})
            (@Html.ActionLink("Edit", "Edit", new {id = client.Id}))
        </li>
    }
</ul>

@Html.ActionLink("New", "Create")

